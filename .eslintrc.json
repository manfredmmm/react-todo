{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "compat"
  ],
  "rules": {
    "comma-dangle": "off",
    "react/sort-comp": "off",
    "no-underscore-dangle": "off",
    "no-unused-expressions": "off"
  },
  "globals": {
    "document": false
  }
}